{
  "manifest_version": 2,

  "name": "Is It Fake",
  "description": "This extension shows a Google Image search result for the current page",
   "content_scripts": [
    {
      "js": ["isitfake_content.js"],
      "matches": ["<all_urls>"]
    }
  ],
  "version": "0.1",

  "background": {
    "scripts": ["isitfake.js"],
    "persistent": false
  },

  "permissions": [
    "activeTab",
    "https://ajax.googleapis.com/"
  ]
}
